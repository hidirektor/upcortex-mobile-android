<resources>
    <declare-styleable name="CircularCountdownView">
        <attr name="progressColor" format="color" />
        <attr name="backgroundColor" format="color" />
        <attr name="progressTextColor" format="color" />
        <attr name="remainingTimeTextColor" format="color" />
        <attr name="progressTextSize" format="dimension" />
        <attr name="remainingTimeTextSize" format="dimension" />
        <attr name="strokeWidth" format="dimension" />
        <attr name="preProgressText" format="boolean" />
        <attr name="progressText" format="string" />
        <attr name="duration" format="integer" />
        <attr name="remainingTime" format="integer" />
        <attr name="circleDiameter" format="dimension" />
        <attr name="clockwise" format="boolean" />
        <attr name="softProgress" format="boolean" />
        <attr name="timeFormat" format="boolean" />
        <attr name="onlySeconds" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CircleStepper">
        <attr name="circleStepperStepSize" format="integer" />
        <attr name="circleStepperCurrentStep" format="integer" />
        <attr name="circleStepperFill" format="boolean" />
        <attr name="circleStepperCircleColor" format="color" />
        <attr name="circleStepperCircleSize" format="dimension" />
        <attr name="circleStepperBorderSize" format="dimension" />
        <attr name="circleStepperCenter" format="boolean" />
        <attr name="circleStepperPadding" format="dimension" />
        <attr name="circleStepperPager" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="HorizontalStepper">
        <attr name="horizontalStepperStepSize" format="integer" />
        <attr name="horizontalStepperCurrentStep" format="integer" />
        <attr name="horizontalStepperCompletedColor" format="color" />
        <attr name="horizontalStepperCurrentColor" format="color" />
        <attr name="horizontalStepperGeneralColor" format="color" />
        <attr name="horizontalStepperLeftTopRadius" format="dimension" />
        <attr name="horizontalStepperLeftBottomRadius" format="dimension" />
        <attr name="horizontalStepperRightTopRadius" format="dimension" />
        <attr name="horizontalStepperRightBottomRadius" format="dimension" />
        <attr name="horizontalStepperPager" format="boolean" />
        <attr name="horizontalStepperPadding" format="dimension" />
    </declare-styleable>

    <declare-styleable name="RatingBar">
        <attr name="srb_borderColor" format="color" />
        <attr name="srb_fillColor" format="color" />
        <attr name="srb_backgroundColor" format="color" />
        <attr name="srb_starBackgroundColor" format="color" />
        <attr name="srb_pressedBorderColor" format="color" />
        <attr name="srb_pressedFillColor" format="color" />
        <attr name="srb_pressedBackgroundColor" format="color" />
        <attr name="srb_pressedStarBackgroundColor" format="color" />
        <attr name="srb_numberOfStars" format="integer" />
        <attr name="srb_starSize" format="dimension" />
        <attr name="srb_maxStarSize" format="dimension" />
        <attr name="srb_starsSeparation" format="dimension" />
        <attr name="srb_starBorderWidth" format="float" />
        <attr name="srb_starCornerRadius" format="float" />
        <attr name="srb_isIndicator" format="boolean" />
        <attr name="srb_rating" format="float" />
        <attr name="srb_stepSize" format="float" />
        <attr name="srb_drawBorderEnabled" format="boolean" />
        <attr name="srb_gravity" format="enum">
            <enum name="left" value="0"/>
            <enum name="right" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="Speedometer">
        <attr name="maxPercent" format="integer" />
        <attr name="borderSize" format="dimension" />
        <attr name="textGap" format="dimension" />
        <attr name="borderColor" format="color" />
        <attr name="fillColor" format="color" />
        <attr name="textColor" format="color" />
        <attr name="tickBorder" format="boolean" />
        <attr name="percentText" format="boolean" />
        <attr name="mode" format="integer" />
        <attr name="firstQuarterColor" format="color" />
        <attr name="secondQuarterColor" format="color" />
        <attr name="thirthQuarterColor" format="color" />
        <attr name="fourthQuarterColor" format="color" />
        <attr name="firstPercent" format="integer" />
        <attr name="secondPercent" format="integer" />
        <attr name="firstPercentColor" format="color" />
        <attr name="secondPercentColor" format="color" />
    </declare-styleable>

    <declare-styleable name="RadialPercent">
        <attr name="baseColor" format="color"/>
        <attr name="percentColor" format="color"/>
        <attr name="currentPercent" format="float"/>
        <attr name="percentStrokeWidth" format="dimension"/>
        <attr name="percentRadius" format="dimension"/>
        <attr name="percentClockwise" format="boolean"/>
        <attr name="percentTextColor" format="color"/>
        <attr name="percentTextFont" format="reference"/>
        <attr name="percentTextSize" format="dimension"/>
        <attr name="percentMax" format="integer" />
    </declare-styleable>
</resources>
